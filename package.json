{
  "name": "fetch-toolkit",
  "version": "0.1.2",
  "description": "Fetch API Toolkit for increased productivity",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "lint": "npx eslint src/**/* --fix",
    "lint-build": "npx eslint src/** --no-error-on-unmatched-pattern",
    "doc": "npx typedoc --plugin typedoc-plugin-markdown --out temp-docs src/index.ts & rimraf temp-docs/README.md & concat-md --toc --decrease-title-levels --dir-name-as-title temp-docs > component.md & rimraf temp-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmarotta/fetch-toolkit.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bmarotta/fetch-toolkit/issues"
  },
  "homepage": "https://github.com/bmarotta/fetch-toolkit#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/jest": "^28.1.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "concat-md": "^0.4.0",
    "eslint": "^8.25.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "jest": "^28.1.2",
    "jest-bamboo-reporter": "^1.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.5",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.6",
    "typedoc-plugin-markdown": "^3.13.3",
    "typescript": "^4.8.4"
  }
}
